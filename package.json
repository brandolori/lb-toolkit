{
  "name": "lb-toolkit",
  "version": "0.10.0",
  "private": true,
  "homepage": "./",
  "main": "vitals/main.js",
  "description": "Utils for personal use",
  "author": {
    "email": "lori.barto@live.it",
    "name": "Lorenzo Bartolini"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "elect-start": "electron .",
    "react-build": "react-scripts build",
    "elect-package": "electron-packager . --overwrite --ignore=\\installer --ignore=\\public --icon=assets\\favicon.ico --win32metadata.requested-execution-level=requireAdministrator",
    "elect-publish": "node ./vitals/generate-installer.mjs",
    "elect-rebuild": "npm rebuild --runtime=electron --target=19.0.0-alpha.1 --dist-url=https://electronjs.org/headers --abi=93",
    "build": "npm run react-build && npm run elect-package && npm run elect-publish"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 2 electron version"
    ],
    "development": [
      "last 2 electron version"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@mantine/core": "^5.2.4",
    "@mantine/hooks": "^5.2.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/iconv": "^3.0.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.54",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "electron": "19.0.14",
    "electron-packager": "^16.0.0",
    "electron-winstaller": "^5.0.0",
    "node-abi": "^3.24.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "dependencies": {
    "execa": "^6.1.0",
    "robotjs": "^0.6.0",
    "wifi-password": "^3.0.1",
    "wifi-qr-code-generator": "^1.1.1"
  }
}